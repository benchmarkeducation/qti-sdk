<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns="http://www.imsglobal.org/xsd/imscp_v1p1"
    xmlns:imsmd="http://www.imsglobal.org/xsd/imsmd_v1p2"
    xmlns:imsqti="http://www.imsglobal.org/xsd/imsqtiasi_v3p0"
    identifier="QTI3_COMPLETE_MANIFEST" version="1.0">
    
    <metadata>
        <schema>IMS Content</schema>
        <schemaversion>1.1.3</schemaversion>
        <imsmd:lom>
            <imsmd:general>
                <imsmd:title>
                    <imsmd:string language="en">Complete QTI 3.0 Test Package</imsmd:string>
                </imsmd:title>
                <imsmd:description>
                    <imsmd:string language="en">A comprehensive QTI 3.0 test package with multiple items and sections</imsmd:string>
                </imsmd:description>
            </imsmd:general>
        </imsmd:lom>
    </metadata>
    
    <organizations default="TOC1">
        <organization identifier="TOC1">
            <title>QTI 3.0 Complete Test Package</title>
            <item identifier="test-item" identifierref="complete-test">
                <title>Complete Assessment Test</title>
                <item identifier="section1-item" identifierref="simple-item">
                    <title>Simple Math Item</title>
                </item>
                <item identifier="section2-item" identifierref="complete-item">
                    <title>Multiple Choice Item</title>
                </item>
            </item>
        </organization>
    </organizations>
    
    <resources>
        <!-- Main assessment test -->
        <resource identifier="complete-test" type="imsqti_test_xmlv3p0" href="sample-qti3-test-complete.xml">
            <file href="sample-qti3-test-complete.xml"/>
            <dependency identifierref="simple-item"/>
            <dependency identifierref="complete-item"/>
        </resource>
        
        <!-- Simple assessment item -->
        <resource identifier="simple-item" type="imsqti_item_xmlv3p0" href="sample-qti3-simple.xml">
            <file href="sample-qti3-simple.xml"/>
        </resource>
        
        <!-- Complete assessment item with response processing -->
        <resource identifier="complete-item" type="imsqti_item_xmlv3p0" href="sample-qti3-item-complete.xml">
            <file href="sample-qti3-item-complete.xml"/>
        </resource>
        
        <!-- Additional resources -->
        <resource identifier="stylesheet" type="text/css" href="qti-styles.css">
            <file href="qti-styles.css"/>
        </resource>
        
    </resources>
    
</manifest>